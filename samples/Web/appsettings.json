{
  // Set to the URL of your machine.
  //"Urls": "http://10.20.30.40:80;https://10.20.30.40:443",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "LettuceEncrypt": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    // Set to the domains you want to get certificates for.
    "DomainNames": [
      [ "test1.domain1.com", "test2.domain1.com" ],
      [ "test1.domain2.com", "test2.domain2.com" ]
    ],
    // Uncomment if you want to use the staging server for testing.
    //"UseStagingServer": true,

    // You must specify an email address to register with the certificate authority.
    // admin@example.com is not valid, use a real adres.
    "EmailAddress": "admin@example.com"
  }
}
